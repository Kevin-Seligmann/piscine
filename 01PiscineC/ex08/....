void ft_sort_int_tab(int *tab, int size);
int check_sort(int size, int *tab);
void swap(int size, int *tab);
#include <stdio.h>

int	main(void)
{
	int a;
	int i = 0;
	int numeros[4] = {96,4,423,0};
	ft_sort_int_tab(&numeros[0], 4);
	printf("%d ", a);
	while(i<4)
	{
		printf("%d ", numeros[i]);
		i++;
	}
	return 0;
}
